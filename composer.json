{
    "name": "nz/portfolio-bundle",
    "type": "symfony-bundle",
    "description": "Symfony Sonata NzPortfolioBundle",
    "keywords": ["sonata", "portfolio"],
    "homepage": "https://nzlabs.es/work/nzportfoliobundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Nadir Zenith",
            "email": "nz@nzlabs.es",
            "homepage": "https://nzlabs.es"
        }
    ],
    "require": {
        "symfony/symfony": "~2.3",
        "sonata-project/admin-bundle": "^3.0",,
        "sonata-project/user-bundle": "~2.2",
        "sonata-project/media-bundle": "~2.2",
        "sonata-project/classification-bundle": "~2.2",
        "sonata-project/formatter-bundle": "~2.3",
        "sonata-project/easy-extends-bundle": "~2.1",
        "sonata-project/intl-bundle": "~2.1",
        "sonata-project/datagrid-bundle": "~2.2",
        "sonata-project/core-bundle": "^2.3.2",
        "friendsofsymfony/rest-bundle": "~1.1",
        "jms/serializer-bundle": "~0.11",
        "nelmio/api-doc-bundle": "~2.4"
    },
    "require-dev": {
        "doctrine/orm": "*",
        "sonata-project/doctrine-orm-admin-bundle": "~2.2",
        "sonata-project/notification-bundle": "~2.2",
        "fabpot/php-cs-fixer": "~0.1|~1.0"
    },
    "conflict": {
        "jms/serializer": "<0.13"
    },
    "suggest": {
        "sonata-project/doctrine-orm-admin-bundle": "~2.2",
        "sonata-project/notification-bundle": "~2.2"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Nz\\PortfolioBundle\\": ""
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.4.x-dev"
        }
    }
}
