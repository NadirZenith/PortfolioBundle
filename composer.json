{
    "name": "nz/portfolio-bundle",
    "type": "symfony-bundle",
    "description": "Symfony Sonata NzPortfolioBundle",
    "keywords": ["sonata", "portfolio"],
    "homepage": "https://nzlabs.es/work/nzportfoliobundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Nadir Zenith",
            "email": "nz@nzlabs.es",
            "homepage": "https://nzlabs.es"
        }
    ],
    "require": {
        "symfony/symfony": "~2.3",
        "sonata-project/admin-bundle": "^3.0",
        "sonata-project/user-bundle": "^3.0",
        "sonata-project/media-bundle": "^3.0",
        "sonata-project/classification-bundle": "^3.0",
        "sonata-project/formatter-bundle": "^3.0",
        "sonata-project/easy-extends-bundle": "~2.1",
        "sonata-project/intl-bundle": "~2.1",
        "sonata-project/datagrid-bundle": "^2.2",
        "sonata-project/core-bundle": "^3.0",
        "friendsofsymfony/rest-bundle": "^1.1",
        "jms/serializer-bundle": "^0.13 || ^1.0"
    },
    "require-dev": {
        "doctrine/orm": "^2.4",
        "sonata-project/doctrine-orm-admin-bundle": "<3.0 || >=4.0",
        "sonata-project/notification-bundle": "^3.0"
    },
    "conflict": {
        "jms/serializer": "<0.13"
    },
    "suggest": {
        "sonata-project/doctrine-orm-admin-bundle": "~2.2",
        "sonata-project/notification-bundle": "~2.2"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Nz\\PortfolioBundle\\": ""
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.4.x-dev"
        }
    }
}
